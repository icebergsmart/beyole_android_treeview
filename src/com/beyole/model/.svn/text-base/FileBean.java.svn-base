package com.beyole.model;

import com.beyole.util.annotation.TreeNodeId;
import com.beyole.util.annotation.TreeNodeLabel;
import com.beyole.util.annotation.TreeNodePid;

public class FileBean {

	//TreeNodeId(String.class)
	@TreeNodeId
	private int id;
	@TreeNodePid
	private int pId;
	@TreeNodeLabel
	private String label;
	
	
	public FileBean(int id, int pId, String label) {
		this.id = id;
		this.pId = pId;
		this.label = label;
	}

	private String desc;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getpId() {
		return pId;
	}

	public void setpId(int pId) {
		this.pId = pId;
	}

	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

}
